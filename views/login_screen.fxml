<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="279.0" prefWidth="432.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.views.loginController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="196.0" prefWidth="424.0" VBox.vgrow="ALWAYS">
      <children>
            <Button fx:id="login_button" layoutX="162.0" layoutY="190.0" mnemonicParsing="false" onAction="#handleLogin" prefHeight="27.0" prefWidth="87.0" text="Login" />
            <Label fx:id="user_name_label" layoutX="75.0" layoutY="64.0" prefHeight="17.0" prefWidth="124.0" text="Username:" />
            <Label fx:id="password_label" layoutX="75.0" layoutY="140.0" prefHeight="17.0" prefWidth="124.0" text="Password" />
            <PasswordField fx:id="password_field" layoutX="205.0" layoutY="135.0" />
            <TextField fx:id="user_name_field" layoutX="205.0" layoutY="59.0" />
      </children>
    </AnchorPane>
  </children>
</VBox>
